[Match]
Name=p2p*
WLANInterfaceType=p2p-go

[Network]
Description=Rocknix Wi-Fi P2P Group Owner
# Standard P2P subnet (Commonly used in Android/Linux P2P)
Address=192.168.49.1/24
DHCPServer=yes
IPMasquerade=ipv4
IPv4Forwarding=yes
# Ensure local name resolution works for the group(either on or avahi seperately running)
MulticastDNS=no
LLMNR=yes

[DHCPServer]
PoolOffset=10
PoolSize=50
EmitDNS=yes
